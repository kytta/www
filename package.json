{
  "name": "karamoff.dev",
  "version": "4.6.1",
  "description": "Nikita Karamov's personal website",
  "license": "MIT",
  "private": true,
  "author": "Nikita Karamov <nick@karamoff.dev>",
  "homepage": "https://karamoff.dev",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/NickKaramoff/website.git"
  },
  "scripts": {
    "build:pug": "pug ./src/index.pug --obj ./src/pugData.js --out ./public",
    "build:sass": "sass --load-path=node_modules/sanitize.css/ ./src/scss/main.scss | postcss -o public/index.css",
    "build:static": "cpy '**/*' '../../public' --cwd=./src/static --parents",
    "build:vavilon": "node ./build/vavilon.js",
    "build": "run-p build:*",
    "start": "sirv ./public --dev"
  },
  "dependencies": {
    "autoprefixer": "^9.8.0",
    "browserslist": "^4.12.0",
    "cpy-cli": "^3.1.1",
    "css-mqpacker": "^7.0.0",
    "cssnano": "^4.1.10",
    "npm-run-all": "^4.1.5",
    "postcss-cli": "^7.1.1",
    "postcss-normalize": "^9.0.0",
    "postcss-preset-env": "^6.7.0",
    "pug": "^3.0.0",
    "pug-cli": "^1.0.0-alpha6",
    "sanitize.css": "^11.0.1",
    "sass": "^1.26.8"
  },
  "devDependencies": {
    "np": "^6.2.3",
    "sirv-cli": "^0.4.6"
  },
  "engines": {
    "node": "12.x"
  },
  "browserslist": [
    ">=.5% and not last 2 versions, last 2 versions",
    "not dead"
  ],
  "np": {
    "publish": false,
    "releaseDraft": false,
    "tests": false
  }
}
